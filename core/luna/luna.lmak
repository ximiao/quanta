--工程名字
PROJECT_NAME = "luna"

--目标名字
TARGET_NAME = "luna"

--工程类型: static/dynamic/exe
PROJECT_TYPE = "static"

--需要的include目录
INCLUDES = {
	"../../extend/luaext/lua/lua"
}

--是否启用mimalloc库
MIMALLOC_DIR = "../../extend/mimalloc/mimalloc/include"

--目标文件前缀
LIB_PREFIX = 1

--需要连接的库文件
LIBS = {
	"lua"
}

--依赖项目
DEPS = {
	"lualib"
}

--分组定义
GROUP = "core"